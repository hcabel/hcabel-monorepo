{
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/portfolio",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nrwl/next:build",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "root": "apps/portfolio",
        "outputPath": "dist/apps/portfolio"
      },
      "configurations": {
        "development": {
          "outputPath": "apps/portfolio"
        },
        "production": {}
      }
    },
    "serve": {
      "executor": "@nrwl/next:server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "portfolio:build",
        "proxyConfig": "apps/portfolio/proxy.conf.json",
        "dev": true
      },
      "configurations": {
        "development": {
          "buildTarget": "portfolio:build:development",
          "dev": true
        },
        "production": {
          "buildTarget": "portfolio:build:production",
          "dev": false,
          "hostname": "10.0.0.78"
        }
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "apps/portfolio/pages/**/*",
          "apps/portfolio/components/**/*",
          "apps/portfolio/styles/**/*",
          "apps/portfolio/3D/**/*"
        ]
      },
      "configurations": {
        "test": {},
        "fix": {
          "fix": true
        }
      }
    }
  },
  "tags": []
}
