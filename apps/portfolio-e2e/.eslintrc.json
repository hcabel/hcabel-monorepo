{
	"extends": [
		"../../.eslintrc.json",
		"plugin:cypress/recommended"
	],
	"plugins": [
		"cypress"
	],
	"parserOptions": {
		"project": [
			"(../../|./)tsconfig.base.json",
			"(apps/portfolio-e2e/|./)tsconfig.json",
			"(apps/portfolio-e2e/|./)tsconfig.spec.json"
		]
	},
	"overrides": [
		{
			"files": [
				"*.cy.ts"
			],
			"rules": {
				"no-unused-expressions": "off"
			}
		}
	]
}